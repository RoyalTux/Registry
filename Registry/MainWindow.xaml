<Window x:Class="Registry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Расширение и совместимость" Height="150" Width="389.966" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label HorizontalAlignment="Left" FontWeight="Bold" Width="92" Margin="0,0,0,88.8">Расширение:</Label>
        <TextBox VerticalContentAlignment="Center" Name="ExtensionTextBox" TextChanged="ExtensionTextBox_TextChanged" Margin="92,0,10.2,88.8"/>

        <Label HorizontalAlignment="Left" FontWeight="Bold" Width="92" Margin="0,32,0,53.8">Программа:</Label>
        <TextBox IsEnabled="False" Name="ProgrammTextBox" VerticalContentAlignment="Center" Margin="92,37,10.2,51.8"></TextBox>

        <Label HorizontalAlignment="Left" FontWeight="Bold" Width="92" Margin="0,67,0,14.8" >Иконка:</Label>
        <TextBox IsEnabled="False" Name="IconTextBox" VerticalContentAlignment="Center" Margin="92,74,10.2,14.8"></TextBox>
    </Grid>
</Window>
